{
  "name": "jibangyoung-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "vercel-build": "npm install --include=optional --include=dev --platform=linux --arch=x64 sharp && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "check": "tsc --noEmit && eslint . --ext .ts,.tsx && prettier --check .",
    "test": "jest",
    "test:unit": "jest --passWithNoTests",
    "test:e2e": "cypress run",
    "cy:open": "cypress open"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^39.0.2",
    "@ckeditor/ckeditor5-react": "^11.0.0",
    "ckeditor5": "^46.0.0",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.6.8",
    "chart.js": "^4.4.1",
    "framer-motion": "^12.1.6",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.323.0",
    "next": "^15.3.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.8",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.19",
    "cypress": "^13.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
